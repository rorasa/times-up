{
  "manifest_version": 2,
  "name": "Time's Up!",
  "version": "1.1.1.23",
  "description": "Help you to stop spending too much time on Facebook!",
  "icons" : {
    "16" : "icon-16.png",
    "32" : "icon-32.png",
    "48" : "icon-48.png",
    "128" : "icon-128.png"
    },
  "background": {
     "page": "background.html",
     "persistent": true
  },
  "options_page": "options.html",
  "page_action" :
  {
    "default_icon" : {
    	"19" : "icon-19.png",
    	"38" : "icon-38.png"
    },
    "default_title" : "Time's Up!",
    "default_popup": "popup.html"
  },
  "permissions" : [
    "tabs",
    "webNavigation",
    "storage",
    "alarms",
    "notifications"
  ]
}
